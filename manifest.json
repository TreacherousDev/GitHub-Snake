{
  "manifest_version": 2,
  "name": "GitHub Snake Game",
  "version": "0.1",
  "description": "Play Snake on your GitHub contribution graph.",
  "permissions": [
    "activeTab",
    "*://github.com/*"
  ],
  "background": {
    "scripts": ["extension-activate.js"]
  },
  "browser_action": {
    "default_title": "Play",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
